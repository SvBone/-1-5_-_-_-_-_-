#include <iostream>
using namespace std;
/*Даны два ненулевых числа. Найти сумму, разность, произведение и
частное их квадратов.*/
int main()
{
	int a, b; cin >> a >> b;
	a *= a; b *= b;
	cout << "amount: "<< a + b << endl;
	cout << "difference: " << a - b << endl;
	cout << "product: " << a * b << endl;
	cout << "quotient: " << float(a) / b << endl;
	system("pause");
	return 0;
}
