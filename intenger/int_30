#include <iostream>
using namespace std;
/*Дан номер некоторого года (целое положительное число). Опре-
делить соответствующий ему номер столетия, учитывая, что, к примеру,
началом 20 столетия был 1901 год.*/
int main()
{
	int K; cin >> K;
	cout << (K / 100) + 1  << endl;
	system("pause");
	return 0;
}
