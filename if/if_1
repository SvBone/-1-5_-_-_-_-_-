#include <iostream>
#include <cmath>
using namespace std;
/*Дано целое число. Если оно является положительным, то прибавить к
нему 1; в противном случае не изменять его. Вывести полученное число.*/
int main()
{
	int x; cin >> x;
	if (x > 0) x += 1;
	cout << x << endl;
	system("pause");
	return 0;
}
