#include <iostream>
using namespace std;
/*Даны три числа. Найти сумму двух наибольших из них.*/
int main()
{
	int a, b, c, t; cin >> a >> b >> c;
	if (a < b)
	{
		t = a;
		a = b;
		b = t;
	}
	if (b < c)
	{
		t = b;
		b = c;
		c = t;
	}
	if (a < b)
	{
		t = a;
		a = b;
		b = t;
	}
	cout << "Result: " << a + b << endl;
	system("pause");
	return 0;
}
