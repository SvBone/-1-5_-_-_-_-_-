#include <iostream>
using namespace std;
/*Даны целые числа a, b, c, являющиеся сторонами некоторого тре-
угольника. Проверить истинность высказывания: «Треугольник со сторо-
нами a, b, c является равносторонним».*/
int main()
{
	int a, b, c; cin >> a >> b >> c;	
	a == b && b == c ? cout << "True" << endl : cout << "False" << endl;
	system("pause");
	return 0;
}
